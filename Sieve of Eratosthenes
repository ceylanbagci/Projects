from termcolor import colored

denomin = [2,3,5,7]




for i in range(1,121):
    if i == 2 or i == 3 or i ==5 or i == 7:
        print(colored(i, 'green'), end="-")
    elif i % 2 == 0 or i % 3 == 0 or i % 5 == 0 or i % 7 == 0:
        print(colored(i,'red'),end="-")

    elif i % 2 != 0 or i % 3 != 0 or i % 5 != 0 or i % 7 != 0:
        print(colored(i,'green'),end="-")

    if i%10==0:
        print("\n")
